{
  "name": "expenses",
  "version": "1.0.0",
  "description": "keep track of expenses",
  "main": "index.js",
  "author": "bopzor",
  "license": "MIT",
  "scripts": {
    "start": "node lib/api/index.js",
    "start:test": "NODE_ENV=test mocha ./lib/tests",
    "build": "babel api -d lib/api",
    "build:test": "babel tests -d lib/tests",
    "build:app": "cd web && yarn build",
    "docker:build": "docker build -t expenses .",
    "docker:run": "docker run -dt --name expenses-container -e NODE_ENV=production -e DB_USER -e DB_PWD -e DB_NAME -e DB_HOST -e PORT=8080 -p 6978:8080 expenses",
    "prod": "sequelize db:migrate && db:seed:all && node lib/api/index.js",
    "sequelize": "sequelize"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.2.0",
    "cors": "^2.8.5",
    "csv": "^5.1.1",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "moment": "^2.24.0",
    "parse-csv": "^0.2.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.1",
    "mocha": "^5.2.0",
    "supertest": "^3.4.2"
  }
}
